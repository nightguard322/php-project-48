{
    "name": "hexlet/code",
    "description": "show difference between two files",
    "autoload": {
        "psr-4": {
            "Differ\\": "src/",
            "Differ\\Differ\\": "src/differ",
            "Differ\\Differ\\Tests\\": "tests/"
        },
        "files": [
            "src/differ/models.php",
            "src/formatters/plain.php",
            "src/formatters/stylish.php",
