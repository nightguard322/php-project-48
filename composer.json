{
    "name": "hexlet/code",
    "description": "show difference between two files",
    "autoload": {
        "psr-4": {
            "Differ\\Differ\\Render\\": "src/differ",
            "Differ\\Differ\\Tests\\": "tests/"
        },
        "files": [
            "src/render/models.php",
            "src/render/plain.php",
            "src/render/stylish.php",
            "src/render/json.php",
            "src/parser.php",
            "src/gendiff.php"
        ]
    },
    "require": {
        "docopt/docopt": "^1.0.4",
        "lstrojny/functional-php" : "^1.17",
        "squizlabs/php_codesniffer" : "^3.7.2"
    },
    "bin": [
        "bin/gendiff"
      ],
    "license": "MIT",
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "symfony/yaml": "6.4.x-dev"
    }
}
